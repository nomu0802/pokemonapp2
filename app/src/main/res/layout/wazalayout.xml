<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/memoview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.057" />
    <TextView
        android:id="@+id/stwaku"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="9dp"
        android:background="@drawable/selectwaku"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/waza1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:text="技1."
        android:textSize="17sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/stwaku" />

    <TextView
        android:id="@+id/waza2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="12dp"
        android:text="技2."
        android:textSize="17sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wazawaku1" />

    <ImageView
        android:id="@+id/waza1line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/wazaline"
        app:layout_constraintStart_toStartOf="@+id/waza1"
        app:layout_constraintTop_toBottomOf="@+id/waza1" />

    <ImageView
        android:id="@+id/waza2line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/wazaline"
        app:layout_constraintStart_toStartOf="@+id/waza2"
        app:layout_constraintTop_toBottomOf="@+id/waza2" />

    <TextView
        android:id="@+id/wazawaku1"
        android:layout_width="0dp"
        android:layout_height="40dp"
android:text="なし"
        android:textColor="#FFF"
        android:gravity="left|center"
        android:textSize="18sp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/wazawaku"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/waza1"
        app:layout_constraintTop_toBottomOf="@+id/waza1line" />

    <TextView
        android:id="@+id/wazawaku2"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:textColor="#FFF"
        android:text="なし"
        android:gravity="left|center"
        android:textSize="18sp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/wazawaku"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/waza2"
        app:layout_constraintTop_toBottomOf="@+id/waza2line" />

    <TextView
        android:id="@+id/waza3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="12dp"
        android:text="技3."
        android:textSize="17sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wazawaku2" />

    <ImageView
        android:id="@+id/waza3line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/wazaline"
        app:layout_constraintStart_toStartOf="@+id/waza3"
        app:layout_constraintTop_toBottomOf="@+id/waza3" />

    <ImageView
        android:id="@+id/waza4line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/wazaline"
        app:layout_constraintStart_toStartOf="@+id/waza4"
        app:layout_constraintTop_toBottomOf="@+id/waza4" />

    <TextView
        android:id="@+id/wazawaku3"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:textColor="#FFF"
        android:gravity="left|center"
        android:textSize="18sp"
        android:text="なし"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/wazawaku"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/waza3"
        app:layout_constraintTop_toBottomOf="@+id/waza3line" />

    <TextView
        android:id="@+id/wazawaku4"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:textColor="#FFF"
        android:gravity="left|center"
        android:textSize="18sp"
        android:text="なし"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="32dp"
        android:background="@drawable/wazawaku"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/waza4"
        app:layout_constraintTop_toBottomOf="@+id/waza4line" />

    <TextView
        android:id="@+id/waza4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="12dp"
        android:text="技4."
        android:textSize="17sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/wazawaku3" />

</androidx.constraintlayout.widget.ConstraintLayout>

